  },
  "devDependencies": {
    "watchify": "~0.5.4",
    "tap": "~0.4.8",
    "browserify-shim": "~3.2.2",
    "uglifyify": "~1.2.1"
  },
  "scripts": {
    "build-js": "browserify -t uglifyify app.js > static/bundle.js",
    "build": "npm run build-js",
    "watch-js": "watchify app.js -o static/bundle.js -dv",
    "watch": "npm run watch-js",
    "test": "tap test/*.js"
  }
