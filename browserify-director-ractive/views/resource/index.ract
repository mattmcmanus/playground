{{#visible}}
  <h1>{{name}} List</h1>
  <table class="table">
    <thead>
      {{#headers}}
        <th>{{this}}</th>
      {{/headers}}
    </thead>
    <tbody>
    {{#resources}}
      <tr>
        <td>{{id}}</td>
        <td><a href="#/network/{{id}}">{{name}}</a></td>
      </tr>
    {{/resources}}
    </tbody>
  </table>
{{/visible}}
